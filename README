  /usr/share/doc/gnome-hideseek-0.5.0/COPYING
  /usr/share/doc/gnome-hideseek-0.5.0/ChangeLog
  /usr/share/doc/gnome-hideseek-0.5.0/INSTALL
  /usr/share/doc/gnome-hideseek-0.5.0/README
  /usr/share/doc/gnome-hideseek-0.5.0/TODO
  /usr/share/doc/gnome-hideseek-0.5.0/example
